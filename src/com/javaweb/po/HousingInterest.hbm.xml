<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- 每个class对应一个持久化对象 -->
 <class name="com.javaweb.po.HousingInterest" table="housing_interest">
  <!-- id元素用来定义主键标识,并指定主键自动递增 -->
  <id name="id">
   <generator class="identity"/>
  </id>
  <!-- 定义其他属性 -->
  <property name="houseId" column="house_id"  type="int"></property>
  <property name="interests" column="interests"></property>

 </class>
</hibernate-mapping>
